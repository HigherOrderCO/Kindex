Mod (a: Nat) (b: Nat) : Nat
Mod  a        b       = (Div_go a b b Nat.zero)

Div_go (a: Nat) (b: Nat) (c: Nat) (z: Nat)     : Nat
Div_go  Nat.zero Nat.zero c        z           = Nat.zero
Div_go  Nat.zero b        c        z           = (Nat.sub c b) 
Div_go  a        Nat.zero c        z           = (Div_go a c c (Nat.succ z)     )
Div_go (Nat.succ a.pred) (Nat.succ b.pred) c z = (Div_go a.pred b.pred c z      )
