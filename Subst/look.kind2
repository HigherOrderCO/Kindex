Subst.look (subst: Subst) (depth: U60): Maybe Term
Subst.look Subst.end             0 = Maybe.none
Subst.look (Subst.unfilled rest) 0 = Maybe.none
Subst.look (Subst.sub term rest) 0 = Maybe.some term
