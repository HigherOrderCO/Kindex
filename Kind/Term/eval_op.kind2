// Evaluates an operation term.
Kind.Term.eval_op (orig: U60) (op: Kind.Operator) (left: Kind.Term) (right: Kind.Term): Kind.Term
Kind.Term.eval_op orig Kind.Operator.add (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (+  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.sub (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (-  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.mul (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (*  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.div (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (/  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.mod (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (%  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.and (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (&  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.or  (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (|  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.xor (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (^  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.shl (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (<< a.num b.num)
Kind.Term.eval_op orig Kind.Operator.shr (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (>> a.num b.num)
Kind.Term.eval_op orig Kind.Operator.ltn (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (<  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.lte (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (<= a.num b.num)
Kind.Term.eval_op orig Kind.Operator.eql (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (== a.num b.num)
Kind.Term.eval_op orig Kind.Operator.gte (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (>= a.num b.num)
Kind.Term.eval_op orig Kind.Operator.gtn (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (>  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.neq (Kind.Term.num60 a.orig a.num) (Kind.Term.num60 b.orig b.num) = Kind.Term.num60 0 (!= a.num b.num)

Kind.Term.eval_op orig Kind.Operator.add (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.add               a.num b.num)
Kind.Term.eval_op orig Kind.Operator.sub (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.sub               a.num b.num)
Kind.Term.eval_op orig Kind.Operator.mul (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.mul               a.num b.num)
Kind.Term.eval_op orig Kind.Operator.div (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.div               a.num b.num)
Kind.Term.eval_op orig Kind.Operator.mod (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.mod               a.num b.num)
Kind.Term.eval_op orig Kind.Operator.and (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.bitwise_and       a.num b.num)
Kind.Term.eval_op orig Kind.Operator.or  (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.bitwise_or        a.num b.num)
Kind.Term.eval_op orig Kind.Operator.xor (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.bitwise_xor       a.num b.num)
Kind.Term.eval_op orig Kind.Operator.shl (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.shift_left        a.num b.num)
Kind.Term.eval_op orig Kind.Operator.shr (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.shift_right       a.num b.num)
Kind.Term.eval_op orig Kind.Operator.ltn (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.num_less_than     a.num b.num)
Kind.Term.eval_op orig Kind.Operator.lte (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.num_less_equal    a.num b.num)
Kind.Term.eval_op orig Kind.Operator.eql (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.num_equal         a.num b.num)
Kind.Term.eval_op orig Kind.Operator.gte (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.num_greater_equal a.num b.num)
Kind.Term.eval_op orig Kind.Operator.gtn (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.num_greater_than  a.num b.num)
Kind.Term.eval_op orig Kind.Operator.neq (Kind.Term.num120 a.orig a.num) (Kind.Term.num120 b.orig b.num) = Kind.Term.num120 0 (U120.num_not_equal     a.num b.num)

Kind.Term.eval_op orig op left right = Kind.Term.op2 orig op left right
