axiom OrigOf (fnid: U60) : U60

Kind.Coverage.check_coverage (fnid: U60) (rules: List Kind.Rule) (type: Kind.Term): Kind.Checker Unit
Kind.Coverage.check_coverage fnid rules type =
  do Kind.Checker {
    ask tree = Kind.Coverage.cover rules type
    let errs = Kind.Coverage.SplitTree.to_errors (OrigOf fnid) fnid tree
    (Kind.Checker.errors errs Unit.new)
  }