Checker/infer (Term/ct15 ctid orig x0) =
  let expr = Checker/infer_args x0
  Checker/infer (expr (Term/ct0 ctid orig) orig)

Checker/infer (Term/ct16 ctid orig x0) =
  let expr = Checker/infer_args x0
  Checker/infer (expr (Term/ct0 ctid orig) orig)

