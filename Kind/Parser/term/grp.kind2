// Parses an application enclosed with ()
Kind.Parser.term.grp (init: U60) : Parser (Maybe Kind.Parser.Term) {
  let guard = Parser.char '('
  let body  = (_: Unit) => do Parser {
    ask term = Kind.Parser.apps
    ask        Kind.Parser.skipping (Parser.char ')')
    return term
  }
  Kind.Parser.term_guard guard body
}