String.split_at (xs : String) (n : Nat) : Pair String String
String.split_at  xs                 Nat.zero    = Pair.new String.nil xs
String.split_at  String.nil        (Nat.succ n) = Pair.new String.nil String.nil
String.split_at (String.cons x xs) (Nat.succ n) =
  match Pair tail = String.split_at xs n {
    new => Pair.new (String.cons x tail.fst) tail.snd
  }