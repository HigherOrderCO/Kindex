Apps.Fib.Interact.pos.aux #Fib_itp_ax (cx: U120) (cy: U120) (tx: U120) (ty: U120) : U120 {
  let func =
    (c => t => 
    if (U120.greater_than c t) {
      (U120.bitwise_or (U120.new 0 8) (U120.sub c t))
    }
    else {
      (U120.sub t c )
    }) :: U120 -> U120 -> U120
  let x = func cx tx
  let y = func cy ty
  U120.bitwise_or x (U120.shift_left y (U120.new 0 4))
}