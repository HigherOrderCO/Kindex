Apps.Fib.Interaction.table.mut (func: Apps.Fib.Table -> Apps.Fib.Table) : Apps.Fib.Interaction Unit {
  (state: Apps.Fib.State) => (player: Apps.Fib.Id) => (entity: Apps.Fib.Id) => (frame: U120) => 
    let table = Apps.Fib.State.get_table state
    let map   = Apps.Fib.State.get_map state
    let state = Apps.Fib.State.new map (func table)
    Apps.Fib.Interaction.Result.new Unit.new state player entity frame
}