use Apps.Kind.Loader as Loader
use Apps.Kind.SyntaxTree.Sugared.Book as Book


Loader/has_name (name: Data.String) : Loader/ Data.Bool {
  state => Loader/has_name.go state name
}

Loader/has_name.go (state: Loader/State) (name: Data.String) : Loader/Result Data.Bool
Loader/has_name.go (Loader/State.new book crnt_id files loaded errs) name =
  let Data.Pair.new (fst=book) (snd=has_name) = Book/has_name name
  let state = Loader/State.new book crnt_id files loaded errs
  Loader/Result.new has_name state