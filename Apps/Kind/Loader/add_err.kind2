use Apps.Kind.Loader as Loader

Loader/add_err (err: Loader/Error) : Loader/ Data.Unit {
  state => Loader/add_err.go state err
}

Loader/add_err.go (state: Loader/State) (err: Loader/Error) : Loader/Result Data.Unit
Loader/add_err.go (Loader/State.new book crnt_id files loaded errs root) err =
  let errs = Data.List.cons err errs
  let state = Loader/State.new book crnt_id files loaded errs root
  Loader/Result.new Data.Unit.new state