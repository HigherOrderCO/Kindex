Term.show (Term.fn0 ctid orig)                          = User.nameOf ctid
Term.show (Term.fn2 ctid orig x0 x1)                    = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) ")"]
Term.show (Term.fn3 ctid orig x0 x1 x2)                 = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) ")"]
Term.show (Term.fn4 ctid orig x0 x1 x2 x3)              = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) " " (Term.show x3) ")"]
Term.show (Term.fn5 ctid orig x0 x1 x2 x3 x4)           = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) " " (Term.show x3) " " (Term.show x4) ")"]
Term.show (Term.fn6 ctid orig x0 x1 x2 x3 x4 x5)        = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) " " (Term.show x3) " " (Term.show x4) " " (Term.show x5) ")"]
