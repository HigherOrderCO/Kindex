User.nameOf (name: U60): String

Term.show.forall (orig: U60) (name: U60) (type: Term) (body: Term -> Term): String
Term.show.forall orig name type body =
  (U60.if (== name 63)
    (Printer.text ["(" (Term.show type) " -> " (Term.show (body (Term.var orig name 0))) ")"])
    (Printer.text ["((" (Name.show name) ": " (Term.show type) ") -> " (Term.show (body (Term.var orig name 0))) ")"]))

Term.show (term: Term): String
Term.show (Term.var orig name index)                    = Printer.text [(Name.show name)]
Term.show (Term.hol orig numb)                          = Printer.text ["_" (Show.to_string (U60.show numb))]
Term.show (Term.typ orig)                               = "Type"
Term.show (Term.all orig name type body)                = Term.show.forall orig name type body
Term.show (Term.lam orig name body)                     = Printer.text ["(" (Name.show name) "=>" (Term.show (body (Term.var orig name 0))) ")"]
Term.show (Term.let orig name exp body)                 = Printer.text ["let " (Name.show name) " = " (Term.show exp) "; " (Term.show (body (Term.var orig name 0)))]
Term.show (Term.ann orig expr type)                     = Printer.text ["{" (Term.show expr) " :: " (Term.show type) "}"]
Term.show (Term.app orig func argm)                     = Printer.text ["(" (Term.show func) " " (Term.show argm) ")"]

Term.show (Term.ct0 ctid orig)                          = User.nameOf ctid
Term.show (Term.ct2 ctid orig x0 x1)                    = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) ")"]
Term.show (Term.ct3 ctid orig x0 x1 x2)                 = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) ")"]
Term.show (Term.ct4 ctid orig x0 x1 x2 x3)              = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) " " (Term.show x3) ")"]
Term.show (Term.ct5 ctid orig x0 x1 x2 x3 x4)           = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) " " (Term.show x3) " " (Term.show x4) ")"]
Term.show (Term.ct6 ctid orig x0 x1 x2 x3 x4 x5)        = Printer.text ["(" (User.nameOf ctid) " " (Term.show x0) " " (Term.show x1) " " (Term.show x2) " " (Term.show x3) " " (Term.show x4) " " (Term.show x5) ")"]
