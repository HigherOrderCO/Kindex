Term.show (Term.hlp orig)                     = "?"
Term.show (Term.u60 orig)                     = "U60"
Term.show (Term.num orig numb)                = Show.to_string (U60.show numb)
Term.show (Term.op2 orig operator left right) = Printer.text ["(" (Operator.show operator) " " (Term.show left) " " (Term.show right) ")"]
