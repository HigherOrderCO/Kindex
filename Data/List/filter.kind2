// Removes all elements of that list that doesn't satisfy a given condition
// Example: (Data.List.filter Data.Nat ([1,2,1,3,1,4] (Data.Nat.equal 1)) -> [1,1,1]
Data.List.filter <a> (xs: Data.List a) (cond: a -> Data.Bool) : Data.List a
Data.List.filter a (Data.List.nil t)            cond = Data.List.nil
Data.List.filter a (Data.List.cons t head tail) cond = Data.List.filter.2 (cond head) head tail cond

Data.List.filter.2 <a> (res: Data.Bool) (head: a) (tail: Data.List a) (cond: a -> Data.Bool) : Data.List a
Data.List.filter.2 Data.Bool.true  head tail cond = Data.List.cons head (Data.List.filter tail cond)
Data.List.filter.2 Data.Bool.false head tail cond = Data.List.filter tail cond