//! /Foo/Bar/Baz.txt -> /Foo/Bar
//! /Foo/Bar/Baz/ -> /Foo/Bar
//! /Foo/Bar/Baz -> /Foo/Bar
//! Baz.txt -> ""
Data.Path.parent (path: Data.Path) : Data.Path {
  Data.Path.parent.go (Data.String.split_last path "/")
}

Data.Path.parent.go (path: Data.Pair Data.String Data.String) : Data.Path
Data.Path.parent.go (Data.Pair.new ""     "") = ""
Data.Path.parent.go (Data.Pair.new parent "") = Data.Path.parent parent
Data.Path.parent.go (Data.Pair.new parent  _) = parent