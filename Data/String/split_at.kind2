use Data.String as String

String/split_at (n: Data.Nat) (xs: String/) : Data.Pair String/ String/ {
  String/split_at.go n xs ""
}

String/split_at.go (n: Data.Nat) (xs: String/) (fst: String/) : Data.Pair String/ String/
String/split_at.go (Data.Nat.succ n) (String/cons x xs) fst = String/split_at.go n xs (String/cons x fst)
String/split_at.go  Data.Nat.zero    (String/cons x xs) fst = Data.Pair.new (String/reverse fst) (String/cons x xs)
String/split_at.go  n                 String/nil        fst = Data.Pair.new (String/reverse fst) String/nil